<script>
    (function () {
        window.requestAnimFrame = (function () {
            'use strict';
            return  window.requestAnimationFrame       ||
                    window.webkitRequestAnimationFrame ||
                    window.mozRequestAnimationFrame    ||
                    window.oRequestAnimationFrame      ||
                    window.msRequestAnimationFrame     ||
                    function (callback) {
                        window.setTimeout(callback, 1000 / 60);
                    };
        })();

        // handle multiple browsers for cancelAnimationFrame() ref:https://msdn.microsoft.com/zh-tw/library/hh920765(v=vs.85).aspx
        window.cancelAnimFrame = (function () {
            return window.cancelAnimationFrame ||
                window.webkitCancelAnimationFrame ||
                window.mozCancelAnimationFrame ||
                window.oCancelAnimationFrame ||
                function (id) {
                    window.clearTimeout(id);
                };
        })();
    })();
</script>

<script>
(() => {
    const getUrlQuery = (query) => {
        const url = new URL(location.href);
        const params = url.searchParams;
        for (let pair of params.entries()) {
            if (pair[0] === query) {
                return pair[1];
            }
        }
        return '';
    };
    const imgAnchorSrc = decodeURIComponent(getUrlQuery('img_anchor'));

    if (!imgAnchorSrc) {
        return;
    }

    const anchorImg = Array.from(document.getElementsByTagName('img'))
        .filter((img) => img.src === imgAnchorSrc || img.dataset.original === imgAnchorSrc)[0];

    if (!anchorImg) {
        return;
    }

    const mainElement = document.getElementById('container');
    mainElement.style.filter = 'blur(2px)';

    const loadingElement = document.createElement('div');
    loadingElement.style.position = 'fixed';
    loadingElement.style.top = '0';
    loadingElement.style.left = '0';
    loadingElement.style.zIndex = '3';
    loadingElement.style.display = 'flex';
    loadingElement.style.justifyContent = 'center';
    loadingElement.style.alignItems = 'center';
    loadingElement.style.width = '100%';
    loadingElement.style.height = '100%';
    loadingElement.style.color = '#fff';
    loadingElement.style.fontSize = '25px';
    loadingElement.style.backgroundColor = 'rgba(0, 0, 0, .6)';
    loadingElement.textContent = 'Searching Image...';
    document.body.appendChild(loadingElement);

    let timer = null;

    const updateState = () => {
        const imgTop = anchorImg.offsetTop;

        if (Math.abs(window.scrollY - imgTop) / imgTop <= .03) {
            clearInterval(timer);
            mainElement.style.filter = '';
            document.body.removeChild(loadingElement);
        }

        window.scrollTo({
            top: imgTop - 80,
            left: 0,
            behavior: 'smooth'
        });
    };

    timer = setInterval(updateState, 250);

    window._piq = window._piq || [];
    window._piq.push(['trackEvent', 'page view', 'line bot', location.href]);
})();
</script>

